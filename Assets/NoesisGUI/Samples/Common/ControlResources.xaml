										<GradientStop Color="#7F4096BF" Offset="0.35"/>
										<GradientStop Color="#B25BB6E2" Offset="0.65"/>
										<GradientStop Color="#004096BF" Offset="1"/>
										<GradientStop Color="#9BDCF4FF" Offset="0.55"/>
