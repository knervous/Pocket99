	</Style>
	<Style x:Key="InvalidCredentialsStyle" TargetType="{x:Type TextBlock}">
		<Style.Resources>
			<Storyboard x:Key="OnEnabled">
				<ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(TextElement.Foreground).(SolidColorBrush.Color)">
					<EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FFF4C861"/>
					<EasingColorKeyFrame KeyTime="0:0:0.4" Value="#FFD49841"/>
					<EasingColorKeyFrame KeyTime="0:0:2.7" Value="#FFD49841"/>
					<EasingColorKeyFrame KeyTime="0:0:3.0" Value="#00D49841"/>
				</ColorAnimationUsingKeyFrames>
			</Storyboard>
		</Style.Resources>
		<Style.Triggers>
			<Trigger Property="IsEnabled" Value="True">
				<Trigger.EnterActions>
					<BeginStoryboard Storyboard="{StaticResource OnEnabled}"/>
				</Trigger.EnterActions>
			</Trigger>
		</Style.Triggers>
		<Setter Property="Foreground" Value="#FFD49841"/>
	</Style>

